{
	"name": "angular-project",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"config": "ts-node ./scripts/setenv.ts",
		"start": "npm run config -- --environment=dev && ng serve",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"compile:server": "webpack --config webpack.server.config.js --progress --colors",
		"serve:ssr": "node dist/server",
		"build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
		"build:client-and-server-bundles": "ng build --prod && ng run angular-project:server:production"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^14.0.0",
		"@angular/common": "^14.0.0",
		"@angular/compiler": "^14.0.0",
		"@angular/core": "^14.0.0",
		"@angular/forms": "^14.0.0",
		"@angular/platform-browser": "^14.0.0",
		"@angular/platform-browser-dynamic": "^14.0.0",
		"@angular/platform-server": "^15.1.5",
		"@angular/router": "^14.0.0",
		"@ngrx/effects": "^14.3.3",
		"@ngrx/router-store": "^14.3.3",
		"@ngrx/store": "^14.3.3",
		"@nguniversal/express-engine": "^7.0.1",
		"@nguniversal/module-map-ngfactory-loader": "^8.2.6",
		"bootstrap": "^3.4.1",
		"core-js": "^2.5.4",
		"express": "^4.15.2",
		"dotenv": "^16.0.3",
		"rxjs": "~7.5.0",
		"ts-node": "^10.9.1",
		"tslib": "^2.3.0",
		"yargs": "^17.5.1",
		"zone.js": "~0.11.4"
