		"@angular/cli": "~14.2.1",
		"@angular/compiler-cli": "^14.0.0",
		"@angular/language-service": "^8.0.0",
		"@ngrx/store-devtools": "^14.3.3",
		"@types/jasmine": "~4.0.0",
		"jasmine-core": "~4.3.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage": "~2.2.0",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.0.0",
		"protractor": "~5.4.0",
		"ts-loader": "^5.2.0",
